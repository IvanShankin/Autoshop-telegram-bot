msgid "Send the user ID/username \n\nNote: It is preferable to use ID, it is more secure"
msgstr "Пришлите ID/username пользователя \n\nПримечание: предпочтительнее использовать ID, так более надёжно"

msgid "User with this ID/Username not found, try again"
msgstr "Пользователь с таким ID/Username не найден попробуйте ещё раз"

msgid "Multiple users with this username were found, please search by ID"
msgstr "Найдено несколько пользователей с данным username, примените поиск по ID"

msgid "Enter a new balance for the user \n\nCurrent: {balance} ₽"
msgstr "Введите новый баланс для пользователя \n\nТекущий: {balance} ₽"

msgid "\n\nReason for ban: {reason}"
msgstr "\n\nПричина бана: {reason}"

msgid "Balance successfully changed \n\nCurrent balance {balance}"
msgstr "Баланс успешно изменён \n\nТекущий баланс {balance}"

msgid "Are you sure you want to unban this user?"
msgstr "Вы уверены, что хотите разблокировать этого пользователя?"

msgid "Please provide a reason for the ban"
msgstr "Укажите причину бана"

msgid "The user has been successfully banned"
msgstr "Пользователь успешно забанен"

msgid "The user has been successfully unbanned"
msgstr "Пользователь успешно разбанен"

msgid "The user has no log entries"
msgstr "У пользователя нет ни одной записи в логах"

