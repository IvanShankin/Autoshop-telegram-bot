msgid "Select service type"
msgstr "Выберите тип сервиса"

msgid "Enter a name for the new service"
msgstr "Введите имя нового сервиса"

msgid "Unable to create. This service type is in use"
msgstr "Невозможно создать. \nДанный тип сервиса занят"

msgid "Service successfully created"
msgstr "Сервис успешно создан"

msgid "Service \n\nName: {name}\nIndex: {index}\nShow: {show}"
msgstr "Сервис \n\nИмя: {name}\nИндекс: {index}\nПоказывать: {show}"

msgid "The service is no longer available"
msgstr "Сервиса больше нет"

msgid "Successfully updated"
msgstr "Успешно обновлён"

msgid "Please enter a new name"
msgstr "Укажите новое имя"

msgid "Name changed successfully"
msgstr "Имя успешно изменено"

msgid "Are you sure you want to delete this service? \n\n"
"Before deleting, make sure that this service does not contain categories!"
msgstr "Вы действительно хотите удалить этот сервис? \n\n"
"⚠️ Перед удаление убедитесь что данный сервис не содержит в себе категории!"

msgid "The service has categories, delete them first"
msgstr "Сервис имеет категории сперва удалите их"

msgid "Service successfully removed!"
msgstr "Сервис успешно удалён!"