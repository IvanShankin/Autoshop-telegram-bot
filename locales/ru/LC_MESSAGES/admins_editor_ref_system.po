msgid "Level: {lvl} \nPercentage of referral's replenishment: {percent} \nLevel achievement amount: {amount_of_achievement}"
msgstr "Уровень: {lvl} \nПроцент от пополнения реферала: {percent}\nСумма достижения уровня: {amount_of_achievement}"

msgid "from {from_sum}₽"
msgstr "от {from_sum}₽"

msgid "from {from_sum}₽ to {to_sum}₽"
msgstr "от {from_sum}₽ до {to_sum}₽"

msgid "Level not found"
msgstr "Уровень не найден"


msgid "Enter the achievement amount for the new level \n\nThe amount must be at least {min_amount}₽"
msgstr "Введите сумму достижения для нового уровня \n\nСумма должна быть не менее {min_amount}₽"

msgid "Enter the percentage of deduction"
msgstr "Введите процент отчисления"


msgid "Enter a new percentage"
msgstr "Введите новый процент"

msgid "Enter a new level achievement amount. \n\n{condition}"
msgstr "Введите новую сумму достижения уровня. \n\n{condition}"

msgid "The new amount must be more than {more}₽ {less}"
msgstr "Новая сумма должна быть более {more}₽ {less}"

msgid "and less {less}₽"
msgstr "и менее {less}₽"

msgid "Incorrect value entered \n\n{condition}"
msgstr "Введено некорректное значение \n\n{condition}"


msgid "Unable to change the achievement amount for the first level"
msgstr "Невозможно изменить сумму достижения для первого уровня"

msgid "The first level cannot be deleted"
msgstr "Первый уровень невозможно удалить"

msgid "Level successfully removed!"
msgstr "Уровень успешно удалёна!"

msgid "Level successfully created!"
msgstr "Уровень успешно Создан!"

msgid "Are you sure you want to delete this level?"
msgstr "Вы действительно хотите удалить этот уровень?"
